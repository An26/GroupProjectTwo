<div class="user-container">
  <div class="enterInfoDivs enterUser">
    <h4>User Info</h4>
    <form class="UserInfo" action="/education" method="POST">
      Name:
      <input type="text" name="fullName" id="fullName" class='form-control'><br>
      Street Address:
      <input type="text" name="street" id="streetAddress" class='form-control'><br>
      City, State, Zip:
      <input type="text" name="rest" id="restAddress" class='form-control'><br>
      Phone Number:
      <input type="text" name="phoneNumber" id="phoneNumber" class='form-control'><br>
      Personal Summary:
      <input type="text" name="summary" id="summary" class='form-control'><br>
      GitHub Profile:
      <input type="text" name="gitHubUrl" id="gitHubUrl" class='form-control'><br>
     <button type="submit" class="btn btn-default">Next</button>
     </form>
  </div>
</div>